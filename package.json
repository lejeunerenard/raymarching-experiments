{
  "name": "raymarch",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "dependencies": {
    "a-big-triangle": "^1.0.3",
    "canvas-fit": "^1.5.0",
    "ccapture.js": "file:../ccapture.js",
    "eases": "^1.0.8",
    "gl-audio-analyser": "^1.0.3",
    "gl-context": "^0.1.1",
    "gl-fbo": "^2.0.5",
    "gl-matrix": "^2.3.2",
    "gl-shader": "^4.2.1",
    "gl-texture2d": "^2.1.0",
    "glsl-checker": "^1.0.1",
    "glsl-easings": "^1.0.0",
    "glsl-fast-gaussian-blur": "^1.0.2",
    "glsl-hsv2rgb": "^1.0.0",
    "glsl-inverse": "^1.0.0",
    "glsl-noise": "0.0.0",
    "glsl-sdf-ops": "0.0.3",
    "glslify": "^7.1.0",
    "lerp": "^1.0.3",
    "octavian": "^2.2.0",
    "raf": "^3.3.0",
    "tween.js": "^16.6.0",
    "webvr-polyfill": "^0.9.24"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^10.0.0",
    "browserify": "^13.1.1",
    "budo": "^11.6.4",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "glslify-hex": "^2.1.1",
    "glslify-import": "^3.0.0",
    "inquirer": "^3.0.6",
    "uglify-js": "^2.7.5"
  },
  "eslintConfig": {
    "extends": "standard",
    "globals": {
      "Audio": true,
      "AudioContext": true,
      "Image": true,
      "dat": true,
      "CCapture": true,
      "XMLHttpRequest": true,
      "WebVRManager": true
    }
  },
  "scripts": {
    "start": "budo simple-index.js:bundle.js --live",
    "build": "npm run build:bundle",
    "build:bundle": "browserify app.js --standalone RayMarch | uglifyjs -cm > bundle.js",
    "build:gallery": "npm run build:bundle; node bin/gallery.js",
    "gh-pages": "git checkout gh-pages; git rebase master; git push -f origin gh-pages; git checkout master",
    "test": "eslint '**/*.js'"
  },
  "keywords": [],
  "author": "Sean Zellmer <sean@lejeunerenard.com> (http://lejeunerenard.com)",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "browserify": {
    "transform": [
      "babelify",
      "glslify"
    ]
  },
  "glslify": {
    "transform": [
      "glslify-import",
      "glslify-hex"
    ]
  }
}
